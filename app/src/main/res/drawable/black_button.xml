<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <solid android:color="#191919" />

    <stroke
        android:width="3dp"
        android:color="#FFD700" />

    <padding
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp" />

    <corners android:radius="10dp" />

    <size
        android:width="20dp"
        android:height="20dp" />

</shape>
